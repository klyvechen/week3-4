<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="org.test.model.TagDetail" table="tag-detail">
		<composite-id>        
            <key-property name="articleId" column="articleId"></key-property>
            <key-property name="tagId" column="tagId" ></key-property>           
        </composite-id>
        <set name="tagDetail" table="tag-detail" lazy="false" cascade="all" inverse="true">
			<key><column name="tagId"/></key>
			<one-to-many class="org.test.model.TagDetail"/>
		</set>   
	</class>
</hibernate-mapping>

