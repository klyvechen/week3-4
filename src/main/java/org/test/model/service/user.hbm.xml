<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="org.test.model.User" table="user">

		<id name="useId" column="userId">
			<generator class="identity" />
		</id>

		<property name="username" column="useername" />
		<property name="password" column="password" />
		<!-- <property name="clubPic" column="clubPic" /> -->		
 		<set name="article" table="article" lazy="false" cascade="all" inverse="true">
			<key><column name="userId"/></key>
			<one-to-many class="org.test.model.Article"/>
		</set>  
	</class>
</hibernate-mapping>

